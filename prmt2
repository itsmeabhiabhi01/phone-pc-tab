<body onload="checkTime();">
    <!-- iPad Interface -->
    <div class="ipad-wrap" id="ipad-wrap">
        <div class="ipadBox">
            <button class="powerButton" onclick="powerMe();">I</button>
            <div class="ipadScreen" onclick="closeSearchOnClick(event)">
                <div class="wallpaper">
                    <div class="statusBar"></div>
                    <div class="search-bar" id="search-bar">
                        <input type="text" id="search-input" placeholder="Search apps...">
                        <button onclick="performSearch()">Search</button>
                    </div>
                    <div class="appGrid" id="appGrid">
                        <!-- YouTube -->
                        <div class="logo" id="ipad-youtube-app">
                            <img src="https://img.icons8.com/?size=100&id=19318&format=png&color=000000" alt="YouTube">
                            <p>YouTube</p>
                        </div>
                        <!-- Games Folder -->
                        <div class="logo" id="ipad-games-app">
                            <svg viewBox="0 0 100 100">
                                <rect x="20" y="30" width="60" height="40" rx="5" fill="#333333" />
                                <rect x="15" y="40" width="5" height="20" rx="2" fill="#222222" />
                                <rect x="80" y="40" width="5" height="20" rx="2" fill="#222222" />
                                <circle cx="35" cy="50" r="10" fill="#444444" stroke="#555555" stroke-width="1" />
                                <circle cx="35" cy="50" r="5" fill="#ff3e3e" />
